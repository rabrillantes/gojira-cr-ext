{
  "manifest_version": 2,

  "name": "JIRA keys",
  "description": "Custom keybindings for JIRA",
  "version": "1.0",

  "background": {
    "scripts": ["background.js"]
  },
  "content_scripts": [
    {
      "matches": ["https://gojira.skyscanner.net/browse/*"],
      "js": ["jira.js"]
    }
  ],
  "permissions": [
    "activeTab",
    "https://gojira.skyscanner.net/*",
    "http://seagull.corp.skyscanner.local/*"
  ]
}
